<%= render 'shared/errors', object: @account %>
<div class="form-group row">
  <div class= "col-md-3">
    <%= f.label :name, t(:name) , class: "required" %>
  </div>
  <div class="col-md-9">
    <%= f.text_field :name , class: "form-control", required: "true", autocomplete: :off %>
  </div>
</div>
<div class="form-group row">
  <div class= "col-md-3 col-form-label">
    <%= f.label :active, t(:active) %>
  </div>
  <div class="col-md-2">
    <%= f.check_box :active, class: "float-left head" %>
  </div>
</div>
